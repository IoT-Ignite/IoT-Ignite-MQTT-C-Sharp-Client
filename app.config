<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!--These credentials can be generated from [Devzone Console](https://devzone.iot-ignite.com/dpanel).-->
    <add key="Username" value="MyUsername" />
    <add key="Password" value="MyPassword" />
    <add key="ClientId" value="MyClientId" />

    <!--These are some sample names.-->
    <add key="Vendor" value="MyVendor" />
    <add key="Nodeid" value="MyNodeid" />
    <add key="Topic1" value="MyTopic1" />
    <add key="Topic2" value="MyTopic2" />
    <add key="Topic3" value="MyTopic3" />
  </appSettings>  
</configuration>